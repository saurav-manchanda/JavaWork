<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
    
 <beans>
 	<bean id="circle" class= "com.bridgelabz.annotations.AutowiredAnnotation.Circle" autowire="byName">
 	</bean>
 	
 	<bean id= "center1" class= "com.bridgelabz.annotations.AutowiredAnnotation.Point">
 	<property name="x" value="0"></property>
 	<property name="y" value="0"></property>
 	</bean>
 	
 	<bean id= "center2" class= "com.bridgelabz.annotations.AutowiredAnnotation.Point">
 	<property name="x" value="20"></property>
 	<property name="y" value="20"></property>
 	</bean>
 	
 	<bean class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor">
 	</bean>
 </beans>